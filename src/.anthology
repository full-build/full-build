apps:
  - name: full-build
    type: copy
    project: fullbuild
  - name: full-build.zip
    type: zip
    project: fullbuild
projects:
  - file: src\FullBuild.Tests\FullBuild.Tests.fsproj
    out: fullbuild.tests.dll
    tests: true
    platform: net452
    packages:
      - id: fsunit
        version: 3.0
      - id: fsharp.core
        version: 4.2.3
    projects:
      - project: fullbuild
  - file: src\FullBuild\FullBuild.fsproj
    out: fullbuild.exe
    tests: false
    platform: net452
    packages:
      - id: fsharp.configuration
        version: 1.3.1
      - id: fsharp.core
        version: 4.2.3
      - id: fsharp.data
        version: 2.3.3
    projects: []
