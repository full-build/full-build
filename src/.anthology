apps:
  - name: full-build
    type: copy
    project: fullbuild
  - name: full-build.zip
    type: zip
    project: fullbuild
projects:
  - file: src\FullBuild.Tests\FullBuild.Tests.fsproj
    out: fullbuild.tests.exe
    tests: true
    packages:
      - id: fsharp.core
        version: 4.2.3
      - id: fsunit
        version: 3.0
      - id: microsoft.net.test.sdk
        version: 15.0.0
      - id: nunit
        version: 3.7.1
      - id: nunit3testadapter
        version: 3.8.0
    projects: []
  - file: src\FullBuild\FullBuild.fsproj
    out: fullbuild.exe
    tests: false
    packages:
      - id: fsharp.configuration
        version: 1.3.1
      - id: fsharp.core
        version: 4.2.3
      - id: fsharp.data
        version: 2.3.3
    projects: []
