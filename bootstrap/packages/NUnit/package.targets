        <Reference Include="nunit.framework">
          <HintPath>$(SolutionDir)/.full-build/packages/NUnit/lib/portable-net45+win8+wp8+wpa81+Xamarin.Mac+MonoAndroid10+MonoTouch10+Xamarin.iOS10/nunit.framework.dll</HintPath>
          <Private>true</Private>
